<h1>Pets</h1>

<% @pets.each do |pet|%>
    <h2>Name: <%= link_to "#{pet.name}", "/pets/#{pet.id}", method: :get %></h2>
    <img src="<%= pet.image %>">
    <h3>Age: <%= pet.age %></h3>
    <h3>Sex: <%= pet.sex %></h3>
    <h3>Shelter: <%=link_to "#{pet.shelter.name}", "/shelters/#{pet.shelter.id}", method: :get %></h3><hr>
<% end %>



    
